<h1>Edit Task</h1>


<%= form_for(@task) do |f| %>
<div>
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>
<div>
  <%= f.label :details %>
  <%= f.text_field :details %>
</div>
<div>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
</div>
<%= f.submit %>
<% end %>
